<Window x:Class="Ozi.Utilities.FmSlider"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TimeChecker" Height="69" Width="601" ResizeMode="NoResize" Topmost="True" WindowStyle="None"
        Background="Black" ShowInTaskbar="False">
    <Grid x:Name="gdMain" Background="#FF383838" Margin="1,0,1,1">
        <Slider x:Name="slTimeChecker" Height="37" Margin="5,10,9,0" VerticalAlignment="Top" Maximum="288"
                SmallChange="1" TickPlacement="Both" TickFrequency="12" LargeChange="288" />
    </Grid>

</Window>